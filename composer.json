{
    "name": "vasilgerginski/filament-accounting",
    "description": "A Filament plugin for managing expenses, incomes and their types",
    "keywords": [
        "laravel",
        "filament",
        "accounting",
        "expenses",
        "incomes",
        "finance"
    ],
    "homepage": "https://github.com/vasilGerginski/filament-accounting",
    "license": "MIT",
    "authors": [
        {
            "name": "Vasil Gerginski",
            "email": "vasil@gerginski.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.0",
        "illuminate/contracts": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Noblehouse\\FilamentAccounting\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Noblehouse\\FilamentAccounting\\FilamentAccountingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}